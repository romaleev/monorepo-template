{
	"compilerOptions": {
		"baseUrl": "./",
		"paths": {
			"#common/*": [
				"../common/*"
			],
			"#server/*": [
				"src/*"
			]
		},
		"target": "esnext",
		"lib": [
			"esnext"
		],
		"allowJs": false,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"noFallthroughCasesInSwitch": true,
		"module": "esnext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"noEmit": false
	},
}